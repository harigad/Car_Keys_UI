<Alloy>
	<Window>
		<ImageView id="bgImage"></ImageView>
		<View top="0" height="60" backgroundColor="#f1f1f1" >
			<View top="10" height="50" >
				<View class="btn_container" onClick="onCancel" left="10" >
					<Label class="btn" >back</Label>
				</View>
				<Label id="question">
					auto shop
				</Label>
			</View>	
		</View>
		<View id="main" >
			<View class="TextFieldContainer">
				<TextField id="plate" hint="where did you get this done?" onFocus="onFocus" onChange="onChange" onBlur="onBlur" ></TextField>
				<Label class="TextFieldLabel" id="plate_label" ></Label>
			</View>
			<TableView id="results" height="300" backgroundColor="#fff"></TableView>
		</View>
	</Window>	
</Alloy>