<Alloy>
	<TableViewRow id="home_square" height="Ti.UI.SIZE" onClick="onClick"  >
		<View height="Ti.UI.SIZE" id="main" left="20" right="10" top="15" bottom="15" >
				<ImageView id="icon" width="50" height="50" left="0"  borderRadius="25" backgroundColor="#eee" ></ImageView>
				<View height="Ti.UI.SIZE" left="60" layout="vertical" id="labels" >
					<View height="Ti.UI.SIZE"  layout="horizontal" >
					  <Label  height="Ti.UI.SIZE" width="Ti.UI.SIZE" id="title" left="0" ></Label>
					  <Label  height="Ti.UI.SIZE" width="Ti.UI.SIZE" id="subtext"  >(200 pts)</Label>
					</View>
					<View  left="0" height="Ti.UI.SIZE" width="Ti.UI.SIZE" id="model" layout="vertical"></View>
				</View>
		</View>
	</TableViewRow>
</Alloy>